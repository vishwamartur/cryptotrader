    "start": "next start",
    "test": "cross-env TEST_TYPE=default jest --testPathPattern=ci.test.ts",
    "test:unit": "cross-env TEST_TYPE=unit jest",
    "test:integration": "cross-env TEST_TYPE=integration jest",
    "test:security": "cross-env TEST_TYPE=security jest",
    "test:performance": "cross-env TEST_TYPE=performance jest",
    "test:uat": "cross-env TEST_TYPE=uat jest",
    "test:all": "cross-env TEST_TYPE=all jest",
    "test:watch": "TEST_TYPE=unit jest --watch",
    "test:coverage": "TEST_TYPE=unit jest --coverage",
    "test:coverage-check": "node scripts/check-coverage.js",
    "test:ci": "TEST_TYPE=default jest --ci --coverage --watchAll=false",
